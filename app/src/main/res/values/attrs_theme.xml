<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="DayNightStyle">
        <attr name="nightTheme" format="reference"/>
        <attr name="dayTheme" format="reference"/>
    </declare-styleable>

    <declare-styleable name="DefaultThemeStyle">
        <attr name="DayNightTheme" format="reference"/>
    </declare-styleable>

    <declare-styleable name="DrawableItem">
        <attr name="name" format="string" />
        <attr name="drawable" format="reference|color" />
    </declare-styleable>

    <declare-styleable name="NightImageView">
        <attr name="src_sets" format="reference" />
        <attr name="current_src" format="string" />
        <attr name="mask" format="boolean" />
    </declare-styleable>

</resources>